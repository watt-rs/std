/// Imports
use std/rt for panic

/// Represents `Option` type
pub enum Option {
  Some(value: dyn),
  None
}

/// Represents unwrap function of option
pub fn unwrap(option: dyn) {
  match option {
    Option.Some(value) -> value
    Option.None -> panic("unwrap on `Option.None`")
  }
}

/// Checks `Option` is `Some`
pub fn is_some(option: dyn): bool {
    match option {
        Option.Some -> true
        Option.None -> false
    }
}

/// Checks `Result` is `Err`
pub fn is_none(option: dyn): bool {
    match option {
        Option.Some -> false
        Option.None -> true
    }
}
